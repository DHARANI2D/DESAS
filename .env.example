# Security Limits
# Limit the file size for uploads (default 10MB)
MAX_UPLOAD_SIZE_BYTES=10485760

# VirusTotal Integration
# Get your API key from: https://www.virustotal.com/gui/user/apikey
VIRUSTOTAL_API_KEY=

# AbuseIPDB Integration (Optional)
# Get your API key from: https://www.abuseipdb.com/account/api
ABUSEIPDB_API_KEY=

# UrlScan.io Integration (Optional)
# Get your API key from: https://urlscan.io/user/signup
URLSCAN_API_KEY=

# MxToolbox Integration
# Get your API key from: https://mxtoolbox.com/
MXTOOLBOX_API_KEY=
